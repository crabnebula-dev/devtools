{
  "identifier": "com.tauri.dev",
  "productName": "Hello World",
  "version": "0.1.0",
  "build": {
    "frontendDist": ["index.html", "test.html"],
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "Welcome to Tauri!",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src 'self'; connect-src ipc: https://ipc.localhost"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      ".icons/32x32.png",
      ".icons/128x128.png",
      ".icons/128x128@2x.png",
      ".icons/icon.icns",
      ".icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool"
  }
}
