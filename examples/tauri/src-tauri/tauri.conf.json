{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "",
    "devPath": "http://localhost:1430",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Hello World",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "dev.crabnebula.devtools",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "example-tauri",
        "width": 800,
        "height": 600
      }
    ]
  }
}
